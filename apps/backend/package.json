		"ua-parser-js": "^2.0.6"
	},
	"devDependencies": {
		"@connected-repo/typescript-config": "*",
		"@types/node": "^22.18.12",
		"@types/pg": "^8.15.5",
		"dotenv": "^17.2.3",
		"pg": "^8.16.3",
		"pino-pretty": "^13.1.2",
		"tsx": "^4.19.2"
